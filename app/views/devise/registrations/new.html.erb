<h2>Sign up new visitor</h2>

<div class="row">
  <div class="span6 offset3">
  	<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  		<%= devise_error_messages! %>
  		<%= f.label :email %>
  		<%= f.text_field :email %>
  		<div><%= f.submit "Sign up", class: "btn btn-large btn-success"  %></div>
  	<% end %>
  </div>
</div>


  


